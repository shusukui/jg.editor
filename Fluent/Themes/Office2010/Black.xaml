                    <Border.OpacityMask>
                      <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                        <GradientStop Color="#3F000000" Offset="0" />
                        <GradientStop Color="#00000000" Offset="0.59" />
