              <Setter Property="Stroke" TargetName="path1" Value="{StaticResource ButtonHoverOuterBorderBrush}" />
              <Setter Property="Fill" TargetName="path" Value="{StaticResource ButtonHoverInnerBackgroundBrush}" />
              <Setter Property="Stroke" TargetName="path" Value="{StaticResource ButtonHoverInnerBorderBrush}" />
